#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    
    while(n--){
        
        int a,b,c;
        cin>>a>>b>>c;
        if(a==b){
            cout<<c<<endl;
        }
       if(b==c){
            cout<<a<<endl;
        }
         if(c==a){
            cout<<b<<endl;
        }
      
    }
    
    
    
}
