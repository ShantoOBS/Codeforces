#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    
    vector<int>v;
    
    for(int i=0;i<n;i++){
        
        int num;
        cin>>num;
        
     v.push_back(num);
    }
    int maxi=v[0];
    int mini=v[0];
    int c=0;
    for(int i=0;i<n;i++){
        if(maxi<v[i]){
            c++;
            maxi=v[i];
        }
      if(mini>v[i]){
            c++;
            mini=v[i];
    }
            
        }
    
    
   cout<<c;
      
        
       
    }
    
    
