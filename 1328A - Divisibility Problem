#include<bits/stdc++.h>
using namespace std;

int recarsive(int a,int b){
    if(a%b==0){
        return a;
    }
    return recarsive(a+1,b);
}
int main()
{
    int n;
    cin>>n;
   
    while(n--){
       int a,b;
       cin>>a>>b;
       if(a%b==0){
           cout<<0<<endl;
       }
       else{
          
           cout<<b-(a%b)<<endl;
       }
        
    }
    
    
}
