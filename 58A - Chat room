#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    cin>>s;
    string a="hello";
    int c=0;
    int j=0;
    for(int i=0;i<s.length();i++){
        if(s[i]==a[j]){
            j++;
            c++;
        }
    }
    if(c==5){
        cout<<"YES";
    }
    else{
        cout<<"NO";
    }
}
