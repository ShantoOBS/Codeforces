#include<bits/stdc++.h>
using namespace std;
int main()
{
   int a,b,c;
   cin>>a;
   cin>>b;
   cin>>c;
   
   int num1=a+b*c;
    int num2=a*(b+c);
     int num3=a*b*c;
       int num4=(a+b)*c;
       int num5=a+b+c;
       vector<int>v;
      v.push_back(num1);
    v.push_back(num2);
     v.push_back(num3);
      v.push_back(num4);
       v.push_back(num5);
      int maxi=num1;
      for(auto u:v){
          maxi=max(maxi,u);
      }
      
      cout<<maxi;
   
}
