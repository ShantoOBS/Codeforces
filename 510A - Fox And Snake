#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,m;
    cin>>n>>m;
   int   temp=1;
    
    for(int i=1;i<=n;i++){
        
        if(i%2==1){
            for(int j=1;j<=m;j++){
            cout<<"#";
        }
        cout<<endl;
        }
        else{
            if(temp%2==1){
            for(int j=1;j<m;j++){
                 cout<<".";
                 if(j+1==m){
                    cout<<"#";
                }
              }
              cout<<endl;
              temp++;
        }
        
         else{
            
              for(int j=1;j<=m;j++){
                if(j==1){
                    cout<<"#";
                }
                else{
                 cout<<".";
                }
              }
              cout<<endl;
            
             temp++;
            
        }
        }
    
       
        
      
    }
}
