// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    
    string s;
    int arr[4];
    int sum=0;
    
    for(int i=0;i<4;i++){
        int t;
        cin>>t;
        arr[i]=t;
    }
    
    cin>>s;
    
    for(int i=0;i<s.length();i++){
        
        int temp=s[i]-'0';
        sum=sum+arr[temp-1];
        
    }
    
    cout<<sum;
   
}
