#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int c=0;
    
    int arr[]={1,2,3,4,5};
    int m=sizeof(arr)/sizeof(int);
    for(int i=m-1;i>=0;i--){
        
        while(arr[i]<=n){
           
            c++;
            n=n-arr[i];
          
        }
        
    }
    
    
    cout<<c;
    
    
}
