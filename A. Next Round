#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,k;
    cin>>n>>k;
    int l=n;
    vector<int>v;
    for(int i=1;i<=n;i++){
        int m;
        cin>>m;
        v.push_back(m);
    }
    int c=0;
    for(int i=0;i<v.size();i++){
    
        if(v[i]>=v[k-1] && v[i]>0){
          
            c++;
        }
    }
    cout<<c;
    
}
