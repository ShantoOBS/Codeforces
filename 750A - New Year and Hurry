#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,t,c=0;
    cin>>n>>t;
    int sum=0;
    for(int i=1;i<=n;i++){
    
        sum=sum+(i*5);
        
    }
    int totaltime=(1200+sum+t);
    
     for(int i=n;i>=0;i--){
    
          if(totaltime<=1440){
             cout<<i;
             break;
            }
            else{
               sum=sum-(i*5);
               totaltime=(1200+sum+t);
                
            }
        
    }
   
    }
    
    
