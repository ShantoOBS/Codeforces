#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[]={4,7,47,74,44,77,444,447,474,477,777,774,747,744};
    int m=sizeof(arr)/sizeof(int);
    bool result=false;
    for(int i=0;i<m;i++){
        if(n%arr[i]==0){
            result=true;
            break;
        }
    }
    if(result){
        cout<<"YES";
        
    }
    else{
        cout<<"NO";
    }
}
